
#include <iostream>
#include <string>
using namespace std;

int main()
{
    int n, i;
    cin >> n;
    string mystring;
    cin >> mystring;
    //Exception case 
    
    if (n <= 2) 
    {
        cout << "-1";
    }
    //Main code
    
    else
    {

        for (i = 2; i < n; i++) 
        {
            if (mystring[i] != mystring[1]) 
                {
                    break;      
                }  
        }
        if (i == n) 
            {
                cout << "-1";
            }
        else
        {
            string lower,higher; 
            lower = mystring;
            higher = mystring;
            if (mystring[n - 1] == '1') 
            {
                lower[n - 1] = '0';
                for (i = n - 1; mystring[i] == '1'; i--)
                    higher[i] = '0';
                higher[i] = '1';
            }
            else if (mystring[n - 1] == '0') 
            {
                higher[n - 1] = '1';
                for (i = n - 1; mystring[i] == '0'; i--)
                    lower[i] = '1';
                lower[i] = '0';
            }
            cout << lower << " " << higher;
        }
    }
}

